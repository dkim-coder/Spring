<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--IContentTypeDAO mybatis-->
<mapper namespace="com.ssafy.live.model.dao.attraction.IContentTypeDAO">

    <resultMap type="ContentType" id="contentTypeResultMap">
        <id property="contentTypeId" column="content_type_id"/>
        <result property="contentTypeName" column="content_type_name"/>
    </resultMap>

    <select id="getAllContentTypes" resultType="ContentType" resultMap="contentTypeResultMap">
        SELECT *
        FROM contenttypes
    </select>

</mapper>
